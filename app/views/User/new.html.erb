<% form_with @event, url: events_path, method: "post", do |f|%>
    <%=f.label "Title"%>
    <%=f.text_field :title%>
    <%=f.label "Location"%>
    <%=f.text_field :location%>
    <%=f.label "Date"%>
    <%=f.text_field :date%>
    <%=f.label "Description"%>
    <%=f.text_area :description%>
<%end%>